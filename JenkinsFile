env.GIT_URL = 'https://github.com/tarikadeepak/serverless-framework.git'

node ('master') {
    stage('NPM Build'){
        sh '''
            ../../../home/ec2-user/.nvm/versions/node/v10.19.0/bin/npm install
        '''
    }
    stage('Deploy on AWS'){
        sh '''
            serverless deploy
        '''
    }
}~/.nvm/versions/node/v10.19.0/bin/npm